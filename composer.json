{
    "name": "muffin/orderly",
    "description": "Default ordering (behavior) for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "orderly"
    ],
    "homepage": "https://github.com/usemuffin/orderly",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/orderly/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/orderly/issues",
        "source": "https://github.com/usemuffin/orderly"
    },
    "require": {
        "cakephp/orm": "3.4.*"
    },
    "require-dev": {
        "cakephp/cakephp": "3.4.*",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Orderly\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Orderly\\Test\\": "tests",
            "Cake\\Test\\Fixture\\": "vendor/cakephp/cakephp/tests/Fixture"
        }
    }
}
